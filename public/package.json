      "popper": "node_modules/popper.js/dist/umd/popper.min.js",
      "marked": "node_modules/marked/lib/marked.js"
    },
    "map": {
      "app/app": "app",
      "basecontrol/basecontrol": "basecontrol",
      "bootstrap/bootstrap": "bootstrap",
      "config/config": "config",
      "cinput/cinput": "cinput",
      "default/default": "default",
      "helpers/helpers": "helpers",
      "jquery/jquery": "jquery",
      "lodash/lodash": "lodash",
      "menu/menu": "menu",
      "pdf/pdf": "pdf",
      "router/router": "router",
      "setup/setup": "setup",
      "start/start": "start",
      "table/table": "table",
      "tablepager/tablepager": "tablepager",
      "tablesorter/tablesorter": "tablesorter",
      "tablewidgets/tablewidgets": "tablewidgets",
      "popper/popper": "popper",
      "marked/marked": "marked"
    },
    "npmAlgorithm": "flat",
    "ext": {
      "css": "steal-css"
    },
    "cssOptions": {
      "timeout": "15"
    },
    "plugins": [
      "steal-css"
    ],
    "bundle": [
      "table",
      "pdf"
    ],
    "meta": {
      "jquery": {
        "exports": "jQuery",
        "format": "global"
      },
      "bootstrap": {
        "deps": [
          "jquery",
          "popper"
        ]
      },
      "tablesorter": {
        "deps": [
          "jquery",
          "bootstrap"
        ]
      },
      "tablepager": {
        "deps": [
          "jquery",
          "bootstrap"
        ]
      },
      "tablewidgets": {
        "deps": [
          "jquery",
          "bootstrap"
        ]
      },
      "popper": {
        "format": "global",
        "exports": "Popper"
      }
    },
    "configDependencies": [
      "live-reload"
    ]
  },
  "browser": {
    "b/app": "./browserify/appl/js/app.js",
    "b/basecontrol": "./browserify/appl/js/utils/base.control.js",
    "b/config": "./browserify/appl/js/config.js",

  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "b/popper": "Popper",
    "bootstrap": {
      "depends": [
        "jquery:jQuery",
        "b/popper:Popper"
      ]
    }
  }
